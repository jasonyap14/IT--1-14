#include <stdio.h>

int main()
{
    float num1,num2, sum, product, quotient, sub;
    printf("Enter a number: ");
    scanf("%f", &num1);
    printf("Enter a number: ");
    scanf("%f", &num2);
    
    sum = num1+num2;
    printf("num1+num2 = %.0f\n", sum);
    
    product = num1*num2;
    printf("num1*num2 = %.0f\n", product);
    
    quotient = num1/num2;
    printf("num1/num2 = %.1f\n", quotient);
    
    sub = num1-num2;
    printf("num1-num2 = %.0f\n", sub);
    
    return 0;
}
